    <table class="table table-striped">
        <thead>
        <tr>
            <th>{{ 'title' | trans({}, 'divers') }}</th>
            <th>{{ 'position' | trans({}, 'divers') }}</th>
            <th>{{ 'enabled' | trans({}, 'divers') }}</th>
            <th>{{ 'edit' | trans({}, 'divers') }}</th>
            <th>{{ 'delete' | trans({}, 'divers') }}</th>
        </tr>
        </thead>
        <tbody>
        {% for rank in rank %}
            <tr>
                <td>{{ rank.title }}</td>
                <td>{% if rank.position == 0  %} 0 {% else %}  {{ rank.position }} {% endif %}</td>
                <td>{% if rank.enabled == 0  %} <i class="glyphicon glyphicon-remove"> </i>  {% else %}  <i class="glyphicon glyphicon-ok"> {% endif %}</td>
                {% if is_granted('ROLE_EDITOR') %}
                    <td><a href="{{ path('rank_edit', { 'id': rank.id }) }}"><i class="glyphicon glyphicon-pencil"></i></a></td>
                {% endif %}
                {% if is_granted('ROLE_ADMIN') %}
                    <td><a href="{{ path('rank_delete', { 'id': rank.id }) }}"><i class="glyphicon glyphicon-trash"></i></a></td>
                {% endif %}
            </tr>
        {% else %}
            <tr>
                <td>{{ 'none' | trans({}, 'divers') }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
