    <table class="table table-striped">
        <thead>
        <tr>
            <th>{{ 'title' | trans({}, 'divers') }}</th>
            <th>{{ 'society' | trans({}, 'divers') }}</th>
            <th>{{ 'diploma' | trans({}, 'divers') }}</th>
            <th>{{ 'email' | trans({}, 'divers') }}</th>
            <th>{{ 'enabled' | trans({}, 'divers') }}</th>
            <th>{{ 'show' | trans({}, 'divers') }}</th>
            <th>{{ 'edit' | trans({}, 'divers') }}</th>
            <th>{{ 'delete' | trans({}, 'divers') }}</th>
        </tr>
        </thead>
        <tbody>
        {% for internship in internship %}
            <tr>
                <td>{{ internship.title }}</td>
                <td>{{ internship.society}}</td>
                <td>{{ internship.diploma}}</td>
                <td>{{ internship.email}}</td>
                <td>{% if internship.enabled == 0  %} <i class="glyphicon glyphicon-remove"> </i>  {% else %}  <i class="glyphicon glyphicon-ok"> {% endif %}</td>
                <td><a href="{{ path('internship_show', { 'id': internship.id }) }}"><i class="glyphicon glyphicon-eye-open"></i></a></td>
                {% if is_granted('ROLE_EDITOR') %}
                    <td><a href="{{ path('internship_edit', { 'id': internship.id }) }}"><i class="glyphicon glyphicon-pencil"></i></a></td>
                {% endif %}
                {% if is_granted('ROLE_ADMIN') %}
                    <td><a href="{{ path('internship_delete', { 'id': internship.id }) }}"><i class="glyphicon glyphicon-trash"></i></a></td>
                {% endif %}
            </tr>
        {% else %}
            <tr>
                <td>{{ 'none' | trans({}, 'divers') }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
